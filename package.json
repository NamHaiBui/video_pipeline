{
  "name": "episode-video-downloader",
  "version": "1.0.0",
  "description": "A video episode downloader to convert YouTube videos to podcast episodes with audio extraction and metadata processing",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "start": "npm run build && node dist/server.js",
    "dev": "tsx src/server.ts",
    "server": "tsx src/server.ts",
    "server:build": "npm run build && node dist/server.js",
    "setup": "tsx src/lib/setup_binaries.ts",
    "postinstall": "tsx src/lib/setup_binaries.ts",
    "update-ytdlp": "tsx src/scripts/update_ytdlp.ts",
    "update-ytdlp:nightly": "tsx src/scripts/update_ytdlp.ts --nightly",
    "update-ytdlp:force": "tsx src/scripts/update_ytdlp.ts --force",
    "update-ytdlp:skip-check": "tsx src/scripts/update_ytdlp.ts --skip-version-check",
    "test:metadata-only": "tsx src/scripts/test-metadata-only.ts",
    "test:slug-filename": "tsx src/scripts/test-slug-filename.ts",
  "test:slug-integration": "tsx src/scripts/test-slug-integration.ts",
  "test:concurrency": "tsx scripts/test-concurrency.ts",
  "test:retry": "tsx scripts/test-retry.ts",
  "test:ecs-protection": "tsx scripts/test-ecs-protection.ts",
  "test:cpu-utilization": "tsx scripts/test-cpu-utilization.ts",
  "show:performance-comparison": "tsx scripts/performance-comparison.ts",
  "backfill:master": "tsx scripts/backfill-master-m3u8.ts",
  "auto-tune": "tsx scripts/auto-tune.ts"
  },
  "keywords": [
    "podcast",
    "audio-processing",
    "youtube",
    "yt-dlp",
    "ffmpeg",
    "node",
    "aws",
    "s3",
    "dynamodb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.830.0",
    "@aws-sdk/client-cloudwatch": "^3.828.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.828.0",
    "@aws-sdk/client-dynamodb": "^3.828.0",
    "@aws-sdk/client-ecs": "^3.828.0",
    "@aws-sdk/client-s3": "^3.828.0",
    "@aws-sdk/client-sqs": "^3.828.0",
    "@aws-sdk/lib-dynamodb": "^3.828.0",
    "@aws-sdk/lib-storage": "^3.844.0",
    "@aws-sdk/s3-request-presigner": "^3.828.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@xmldom/xmldom": "^0.9.8",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dateutil": "^0.1.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "mysql2": "^3.14.1",
    "node-fetch": "^3.3.2",
    "openai": "^5.8.3",
    "pg": "^8.16.3",
    "progress": "^2.0.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.2",
    "@types/node": "^22.15.29",
    "@types/pg": "^8.15.4",
    "@types/progress": "^2.0.7",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
